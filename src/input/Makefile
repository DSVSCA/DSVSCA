DSVSCA:
	g++ "../main.cpp" decomposer.cpp -lavformat -lavcodec -lavfilter -lavutil -lpthread -lswresample -lswscale -o ../../build/DSVSCA

debug:
	g++ -g "../main.cpp" decomposer.cpp -lavformat -lavcodec -lavfilter -lavutil -lpthread -lswresample -lswscale -o ../../build/DSVSCA_debug

clean:
	rm -f ../../build/DSVSCA_debug ../../build/DSVSCA
